  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=prod&& electron .",
    "test": "set NODE_ENV=dev&& electron .",
    "publish": "electron-builder -w --publish always",
    "build": "electron-builder -w"
  },
  "build": {
    "appId": "com.pixeldev.notepage",
