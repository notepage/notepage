    "electron-log": "^4.2.2",
    "electron-prompt": "^1.5.1",
    "electron-updater": "^4.3.1",
    "etl": "^0.6.12",
    "fs": "*",
    "hotkeys-js": "^3.8.1",
    "html": "^1.0.0",
    "html-pdf": "^2.2.0",
